{
  "env": {
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins" : [
    "prettier"
  ],
  "rules": {
    "eqeqeq": [2, "smart"],
    "new-cap": 2,
    "no-cond-assign": 0,
    "no-console": 0,
    "no-use-before-define": 2,
    "one-var": [2, "never"]
  }
}
